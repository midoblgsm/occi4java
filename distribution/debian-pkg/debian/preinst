#!/bin/sh

set -e

if [ ! -d /usr/share/occi4java ]; then
   	mkdir /usr/share/occi4java
fi
if [ ! -d /var/log/occi4java ]; then
   	mkdir /var/log/occi4java
fi
if [ ! -h /usr/share/occi4java/conf ]; then
    	ln -s /etc/occi4java/conf /usr/share/occi4java/conf
fi

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0